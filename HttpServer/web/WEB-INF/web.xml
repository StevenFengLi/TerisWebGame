<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.LoginRegist.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RegistServlet</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.LoginRegist.RegistServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetUserInformation</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.UserInformation.GetUserInformation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetUserInformation</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.UserInformation.SetUserInformation</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddFriend</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Friends.AddFriend</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetFriends</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Friends.GetFriends</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CreateGroup</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Group.CreateGroup</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddMember</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Group.AddMember</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetGroupInformation</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Group.GetGroupInformation</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SendSquareMessage</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.SendSquareMessage</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetSquareMessagesBeforeTimeInPoint</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.GetSquareMessagesBeforeTimeInPoint</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetSquareMessagesAfterTimeInPoint</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.GetSquareMessagesAfterTimeInPoint</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GetComments</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.GetComments</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SendComment</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.SendComment</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteFriend</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Friends.DeleteFriend</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>QuitGroup</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Group.QuitGroup</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>QuitGroupByMaster</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Group.QuitGroupByMaster</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteSquareMessage</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.DeleteSquareMessage</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DeleteComment</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.Square.DeleteComment</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>HTTPServer.RequestProcessingLayer.test</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>



    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/user/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegistServlet</servlet-name>
        <url-pattern>/user/register</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetUserInformation</servlet-name>
        <url-pattern>/user/info</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SetUserInformation</servlet-name>
        <url-pattern>/user/setinformation</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddFriend</servlet-name>
        <url-pattern>/friends/add</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>GetFriends</servlet-name>
        <url-pattern>/friends/all</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CreateGroup</servlet-name>
        <url-pattern>/groupChat/create</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddMember</servlet-name>
        <url-pattern>/groupChat/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetGroupInformation</servlet-name>
        <url-pattern>/groupChat/information</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SendSquareMessage</servlet-name>
        <url-pattern>/square/sendSquareMessage</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetSquareMessagesBeforeTimeInPoint</servlet-name>
        <url-pattern>/square/getSquareMessagesBeforeTimeInPoint</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetSquareMessagesAfterTimeInPoint</servlet-name>
        <url-pattern>/square/getSquareMessagesAfterTimeInPoint</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetComments</servlet-name>
        <url-pattern>/square/getComments</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SendComment</servlet-name>
        <url-pattern>/square/sendComment</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteFriend</servlet-name>
        <url-pattern>/friends/delete</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>QuitGroup</servlet-name>
        <url-pattern>/group/QuitGroup</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>QuitGroupByMaster</servlet-name>
        <url-pattern>/group/quitGroupByMaster</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteSquareMessage</servlet-name>
        <url-pattern>/square/deleteSquareMessage</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DeleteComment</servlet-name>
        <url-pattern>/square/deleteComment</url-pattern>
    </servlet-mapping>
</web-app>


